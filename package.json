        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.9.4",
        "babel-loader": "^8.1.0",
        "babel-plugin-lodash": "^3.3.4",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.1.0",
        "css-loader": "^4.3.0",
